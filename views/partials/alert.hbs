{{#if success_msg}}
<script>
    Swal.fire({
        title: 'Error!',
        text: '{{ success_msg }}',
        icon: 'success',
    })
</script>
{{/if}}


{{#if error_msg}}

<script>
    Swal.fire({
        title: 'Error!',
        text: '{{error_msg}}',
        icon: 'error'
    })
</script>
{{/if}}